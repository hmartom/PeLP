        <parameter name="SpringBeanName" locked="false">adminService</parameter>        
        <operation name="addAdministrator">
            <messageReceiver class="org.apache.axis2.rpc.receivers.RPCMessageReceiver" />
        </operation>
        <operation name="deleteAdministrator">
            <messageReceiver class="org.apache.axis2.rpc.receivers.RPCMessageReceiver" />
